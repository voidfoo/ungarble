{
  "manifest_version": 3,
  "name": "Fix broken CJK text",
  "version": "1.1",
  "description": "See all transformations and pick the one you want",

  "icons": { "128": "icon.png" },

  "action": {
    "default_popup": "popup.html",
    "default_title": "Transform text"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["activeTab", "scripting"]
}
